Algoritmo "Quantos anos eu tenho?"
Var
   ano, nascimento,idade: inteiro
Inicio
      Escreva("Em que ano nós estamos?")
      Leia(ano)
      Escreva("Em que ano eu nasci?")
      Leia(nascimento)
      ano<-ano
      nascimento<-nascimento
      idade<-ano-nascimento
      Escreva("sua idade é",idade,"!")
FimAlgoritmo
