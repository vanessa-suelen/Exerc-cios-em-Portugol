Algoritmo "ConteAteOndeOUsuarioQuiser"

Var
   valor, contador: inteiro

Inicio
   contador <- 0
   Escreva("Quer contar até quanto?")
   Leia (valor)
   Enquanto (contador <= valor) faca
      EscrevaL (contador)
      contador <- contador + 1
   FimEnquanto
   Escreval("TERMINEI DE CONTAR")

Fimalgoritmo